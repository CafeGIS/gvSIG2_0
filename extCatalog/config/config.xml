<?xml version="1.0" encoding="ISO-8859-1"?>
<plugin-config>
	<depends plugin-name="com.iver.cit.gvsig"/>
	<libraries library-dir="lib"/>
	<resourceBundle name="text"/>
	<extensions>
		<extension class-name="org.gvsig.catalog.CatalogClientExtension"
			description="Catalog extension"
			active="true" 
			priority="3">
			<tool-bar name="Catalogo">
				<action-tool icon="catalog-search" 
					tooltip="catalog_search"
					position="1"
				/>
			</tool-bar>		
		</extension>
		<extension class-name="org.gvsig.catalog.CatalogClientAboutExtension"
			description="About panel for the catalog and gazetteer clients" 
			active="true" 
			priority="3">
		</extension>
	</extensions>
</plugin-config>
